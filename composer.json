{
    "name": "chrizus/php-spotify-web-api",
    "description": "A PHP wrapper for Spotify's Web API.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "spotify"
    ],
    "homepage": "https://github.com/chrizus/php-spotify-web-api",
    "authors": [
        {
            "name": "Jonathan Wilsson",
            "email": "jonathan.wilsson@gmail.com"
        },
        {
            "name": "Chris van den Berg",
            "email": "chrizus@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "ext-curl": "*"
    },
    "require-dev": {
        "php-coveralls/php-coveralls": ">=2.5",
        "php-mock/php-mock-phpunit": ">=2.7",
        "phpunit/phpunit": ">=10.2",
        "squizlabs/php_codesniffer": ">=3.0"
    },
    "autoload": {
        "psr-4": {
            "SpotifyWebAPI\\": "src/"
        }
    },
    "scripts": {
        "test": "phpcs src -v && phpunit"
    }
}
